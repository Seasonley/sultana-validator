{
  "name": "sultana-validator",
  "version": "1.0.0",
  "description": "A library for validating as same as validator.py",
  "main": "src/index.js",
  "files": [
    "src/index.js",
    "README.md",
    "LICENCE"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jsdoc-to-markdown": "^4.0.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^3.1.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "minify": "uglifyjs sultana-validator.js -o sultana-validator.min.js --compress --mangle --comments /Copyright/",
    "build": "rollup --format umd --config rollup.config.js && npm run minify",
    "pretest": "npm run lint && npm run build",
    "test": "nyc mocha --reporter dot",
    "test:browser": "karma start",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov mocha test/*.js",
    "coveralls":"cat ./coverage/lcov.info | coveralls",
    "changelog": "conventional-changelog -p angular -i doc/CHANGELOG.md -s -r 0",
    "makedoc": "jsdoc2md src/index.js > doc/DOCUMENTATION.md",
    "doc": "npm run makedoc && npm run changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seasonley/sultana-validator.git"
  },
  "keywords": [
    "validator"
  ],
  "author": "seasonley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seasonley/sultana-validator/issues"
  },
  "homepage": "https://github.com/Seasonley/sultana-validator",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}