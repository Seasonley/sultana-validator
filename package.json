{
  "name": "sultana-validator",
  "version": "0.0.2",
  "description": "A library for validating as same as validator.py",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md",
    "LICENCE",
    "sultana-validator.js",
    "sultana-validator.min.js"
  ],
  "dependencies": {
    "rollup-plugin-babel": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^3.1.0",
    "should": "^13.2.3"
  },
  "scripts": {
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "minify": "uglifyjs sultana-validator.js -o sultana-validator.min.js --compress --mangle --comments /Copyright/",
    "build:umd": "rollup --config rollup.config.js",
    "build:node": "babel src -d .",
    "build": "npm run build:umd && npm run minify",
    "pretest": "npm run lint && npm run build",
    "test:node": "mocha --require @babel/register --reporter dot",
    "test:browser":"karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seasonley/validator.js.git"
  },
  "keywords": [
    "validator"
  ],
  "author": "seasonley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seasonley/validator.js/issues"
  },
  "homepage": "https://github.com/Seasonley/validator.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
